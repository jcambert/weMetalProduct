<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="assets_backend" name="my_style" inherit_id="website.assets_backend">
        <xpath expr="link[last()]" position="after">
            <link rel="stylesheet" type="text/scss" href="/weMetalProduct/static/scss/assets.scss"/>
        </xpath>
    </template>
    <template id="_assets_primary_variables" name="My SCSS variables" inherit_id="website._assets_primary_variables">
        <xpath expr="link[last()]" position="after">
            <link rel="stylesheet" type="text/scss" href="/weMetalProduct/static/scss/primary_variables.scss"/>
        </xpath>
    </template>
</odoo>