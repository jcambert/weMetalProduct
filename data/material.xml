<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data >
        <record forcecreate="True" model="we.material" id="material_steel">
            <field name="name">Acier</field>
            <field name="volmass">7.8</field>
            <field name="convention" >^(?P&lt;name&gt;(s|e)[0-9]{3}|(dc|dd|xc)[0-9]{2}|dx51d)$</field>
            <field name="default" >1</field>
        </record>
        <record forcecreate="True" model="we.material" id="material_stainlesssteel">
            <field name="name">Inox</field>
            <field name="volmass">7.8</field>
            <field name="convention" >^(?P&lt;name&gt;[0-9]{3}[l]{0,1})$</field>
        </record>
        <record forcecreate="True" model="we.material" id="material_alu">
            <field name="name">Aluminium</field>
            <field name="volmass">2.7</field>
            <field name="convention">^(?P&lt;name&gt;[0-9]{4})$</field>
        </record>
        <record forcecreate="True" model="we.material" id="material_cuivre">
            <field name="name">Cuivre</field>
            <field name="volmass">8.95</field>
            <field name="convention">^(?P&lt;name&gt;CU)$</field>
        </record>
        <record forcecreate="True" model="we.material" id="material_titane">
            <field name="name">Titane</field>
            <field name="volmass">4.5</field>
            <field name="convention">^(?P&lt;name&gt;TI)$</field>
        </record>
        <record forcecreate="True" model="we.material" id="material_fonte">
            <field name="name">Fonte</field>
            <field name="volmass">7.1</field>
            <field name="convention"></field>
        </record>

    </data>
</odoo>